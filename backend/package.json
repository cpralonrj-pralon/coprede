{
    "name": "coprede-backend",
    "version": "0.0.1",
    "description": "Backend for COp Rede Dashboard",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\"",
        "start": "ts-node src/main.ts",
        "start:dev": "nodemon --watch src --exec ts-node src/main.ts",
